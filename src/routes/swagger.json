    "/usuarios/": {
      "post": {
        "tags": [
          "Usuario"
        ],
        "summary": "Cadastrar Usuario",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Adiciona um novo Usuario",
            "schema": {
              "type": "object",
              "properties": {
                "nome": {
                  "type": "string",
                  "example": "Seu nome"
                },
                "sexo": {
                  "type": "string",
                  "example": "feminino, masculino ou outro"
                },
                "cpf": {
                  "type": "string",
                  "example": "12345678901"
                },
                "data_nascimento": {
                  "type": "string",
                  "example": "aaaa-mm-dd"
                },
                "cep": {
                  "type": "string",
                  "example": "12345678"
                },
                "endereco": {
                  "type": "string",
                  "example": "seu endereço"
                },
                "numero": {
                  "type": "number",
                  "example": 0
                },
                "complemento": {
                  "type": "string",
                  "example": "# do seu apartamento"
                },
                "email": {
                  "type": "string",
                  "example": "teste123@gmail.com"
                },
                "password": {
                  "type": "string",
                  "example": "sua senha"
                }
              },
              "required": [
                "nome",
                "sexo",
                "cpf",
                "data_nascimento",
                "cep",
                "numero",
                "complemento",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
